{
  "name": "@paybutton/react",
  "version": "2.1.2",
  "description": "The easiest way to accept eCash online",
  "author": "poldridge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PayButton/paybutton.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --jsx React.createElement --format modern,cjs",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier --write 'src/**/*.ts'",
    "dev": "concurrently yarn:watch yarn:storybook",
    "storybook": "start-storybook -p 6006",
    "watch": "microbundle watch --jsx React.createElement --no-compress --format modern,cjs",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "17.0.0",
    "react-dom": "17.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@popperjs/core": "2.11.5",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/node-logger": "6.5.9",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.5.9",
    "@types/currency-formatter": "1.5.1",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.41",
    "@types/qrcode.react": "1.0.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "concurrently": "5.3.0",
    "cross-env": "7.0.3",
    "currency-formatter": "1.5.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-react": "9.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-standard": "4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "microbundle": "0.12.4",
    "yarn-run-all": "^3.1.1",
    "prettier": "2.7.1",
    "react-is": "16.13.1",
    "react-scripts": "3.4.4",
    "typescript": "^4.8.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@material-ui/styles": "4.11.5",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "crypto-js": "^4.2.0",
    "@types/crypto-js": "^4.2.1",
    "ts-jest": "^29.1.1",
    "axios": "1.6.5",
    "bignumber.js": "9.0.2",
    "copy-to-clipboard": "3.3.3",
    "lodash": "4.17.21",
    "notistack": "3.0.1",
    "qrcode.react": "1.0.1",
    "react-jss": "10.10.0",
    "socket.io-client": "4.7.4",
    "xecaddrjs": "^0.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0"
  }
}
